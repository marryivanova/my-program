array = [2, 4, 6, 8]
res = []

for i in array:
    row = []
    for j in range(1, 8):
        row.append(i * j)
    res.append(row)

# Вывод таблицы умножения
for row in res:
    print(row)
    
    
#или
array = []
multiplier = 1, 7
for i in range(len(array)):
    array[i] *= multiplier
print(array)  
array = [x * multiplier for x in array]

print(array)
    
    
    
    
