class Point:
    def __init__(self, x, y):
        self.x = x
        self.y = y
    
    def move(self, bx, by):
        self.x += bx
        self.y += by
    
    def length(self, point):
        distance = ((self.x - point.x) ** 2 + (self.y - point.y) ** 2) ** 0.5
        return round(distance, 2)
