def solution(sequence):
    for i in range(len(sequence)-1):
        if sequence[i] > sequence[i+1]:
            return False
    return True


def solution(sequence):
    if len(sequence) != len(set(sequence)):
        return False
    for i in range(len(sequence)-1):
        if sequence[i] > sequence[i+1]:
            return False
    return True
    
    
